using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace IndexerCode
{
    public class Employee
    {
        public int ID { get; set; }
        public string Name { get; set; }
        public string Job { get; set; }
        public double Salary { get; set; }
        public string Location { get; set; }
        public string Department { get; set; }
        public string Gender { get; set; }
        public Employee(int ID, string Name, string Job, int Salary, string Location,
                        string Department, string Gender)
        {
            this.ID = ID;
            this.Name = Name;
            this.Job = Job;
            this.Salary = Salary;
            this.Location = Location;
            this.Department = Department;
            this.Gender = Gender;
        }
    }
    class Test
    {
        public static void Main(String[] args)
        {
            Employee emp = new Employee(10, "Deepak", "Software Developer", 100000, "Texas", "IT", "Male");
            Console.WriteLine(emp.ID);
            Console.WriteLine(emp.Name);
            Console.WriteLine(emp.Job);
            Console.WriteLine(emp.Salary);
            Console.WriteLine(emp.Location);
            Console.WriteLine(emp.Department);
            Console.WriteLine(emp.Gender);
        }
    }
}
